.tab-bar {
  border-top-left-radius: 60rpx;
  border-top-right-radius: 60rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  background: white;
  display: flex;
  padding: 18rpx 0;
}
.tab-bar-item {
  flex: 1;
  text-align: center;
}
.tab-bar-item .CoverImg{
  width: 48rpx;
  height: 48rpx;
}
.tab-bar-item .view_item{
  font-size: 28rpx;
}
