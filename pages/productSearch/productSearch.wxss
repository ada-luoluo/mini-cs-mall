.search-content{
  background-color: #fff;
  padding: 30rpx;
}
.searchDiv{
  background: #F6F6F6;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 8rpx 30rpx;
  border-radius: 80rpx;
}
.searchDiv .image1{
  height: 40rpx;
  width: 40rpx;
}
.searchDiv .inputs{
  height: 70rpx;
  width: 65%;
  font-size: 28rpx;
}
.searchDiv .image2{
  width: 32rpx;
  height: 32rpx;
}
.search-btn{
  width: auto!important;
  padding: 0;
  color: #3383FF;
  font-weight: inherit;
  background: none;
  font-size: 28rpx;
  margin: 0!important;
  border-left: 1px solid #D9D9D9;
  padding-left: 30rpx;
  border-radius: 0;
}
.searchItem{
  padding: 24rpx 100rpx;
}
.searchItem image{
  width: 40rpx;
  height: 40rpx;
  margin-right: 16rpx;
}
.searchItem image,.searchItem text{
  display: inline-block;
  vertical-align: middle;
}

.autocomplete-item{
   padding-top: 30rpx;
}
